{
    "version": 2,
    "builds": [
      {
        "src": "*.php",
        "use": "@vercel/php"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "api/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.php"
      }
    ]
  }